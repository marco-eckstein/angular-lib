{
  "name": "angular-lib-app",
  "version": "2.0.2",
  "scripts": {
    "start": "ng serve --open",
    "build-lib": "ng build @marco-eckstein/angular-lib-common --prod",
    "build-app": "ng build angular-lib-app --prod",
    "build": "npm run build-lib && npm run build-app",
    "test-lib": "ng test @marco-eckstein/angular-lib-common",
    "test-app": "ng test angular-lib-app",
    "test": "ng test @marco-eckstein/angular-lib-common --watch=false && ng test angular-lib-app --watch=false",
    "lint-lib": "ng lint @marco-eckstein/angular-lib-common",
    "lint-app": "ng lint angular-lib-app",
    "lint": "npm run lint-lib && npm run lint-app",
    "e2e": "ng e2e",
    "doc": "./node_modules/.bin/compodoc -p ./tsconfig.json",
    "verify": "npm run build && npm run lint && npm test && npm run e2e && npm run doc",
    "standard-version": "git add --all && standard-version --commit-all",
    "publish-lib": "npm run verify && npm publish ./dist/marco-eckstein/angular-lib-common --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@compodoc/compodoc": "^1.1.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "standard-version": "^4.4.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.38.0",
    "tslint": "^6.0.0",
    "typescript": "~3.7.5"
  }
}
